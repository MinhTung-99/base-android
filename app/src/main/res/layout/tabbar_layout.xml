        android:layout_height="match_parent"
        android:layout_weight="1"
        android:tag="2"
        app:selectedIcon="@drawable/ic_mail_selected"
        app:myIcon="@drawable/ic_email"/>

    <com.develop.base_android.application.base.tabbar.TabbarItemView
